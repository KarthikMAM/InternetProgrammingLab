<web-app>
    <servlet>
        <servlet-name>login</servlet-name>
        <jsp-file>/WEB-INF/jsp/login.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login.jsp</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>trans</servlet-name>
        <jsp-file>/WEB-INF/jsp/trans.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>trans</servlet-name>
        <url-pattern>/trans.jsp</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>process</servlet-name>
        <jsp-file>/WEB-INF/jsp/process.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>process</servlet-name>
        <url-pattern>/process.jsp</url-pattern>
    </servlet-mapping>
</web-app>
